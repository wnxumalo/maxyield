<?xml version='2.0' encoding='utf-8'?>
<widget id="com.erpbyw.maxyield" version="2.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>MaxYield</name>
    <description>
        Crop Yield Maximisation Software
    </description>
    <author email="info@machuzu.com" href="http://www.machuzu.com">
        Wandile Nxumalo
    </author>
<icon src="www/img/maxyieldlogo.png" />
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>

</widget>
